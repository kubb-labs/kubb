{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "separate-pull-requests": false,
  "changelog-sections": [
    {
      "type": "feat",
      "section": "‚ú® Features"
    },
    {
      "type": "feature",
      "section": "‚ú® Features"
    },
    {
      "type": "fix",
      "section": "üêû Bug Fixes"
    },
    {
      "type": "perf",
      "section": "üöÄ Performance Improvements"
    },
    {
      "type": "revert",
      "section": "Reverts"
    },
    {
      "type": "docs",
      "section": "üìö Documentation"
    },
    {
      "type": "style",
      "section": "Styles",
      "hidden": true
    },
    {
      "type": "chore",
      "section": "üì¶ Miscellaneous Chores",
      "hidden": true
    },
    {
      "type": "refactor",
      "section": "Code Refactoring",
      "hidden": true
    },
    {
      "type": "test",
      "section": "Tests",
      "hidden": true
    },
    {
      "type": "ci",
      "section": "Continuous Integration",
      "hidden": true
    }
  ],
