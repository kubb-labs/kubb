{
  "prettier.configPath": "configs/prettier.config.cjs",
  "editor.defaultFormatter": "dprint.dprint",
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.awcache": true,
    "**/.husky": true,
    "**/.tscache": true,
    "**/.gitattributes": true,
    "**/test-results": true,
    "*.babelcache": true,
    "**/*.babelcache": true,
    "*.tersercache": true,
    "certificates": true,
    "nginx": true,
    "yarn-error.log": true,
    "*.tmp.txt": true,
    "**/.turbo": true,
    "**/.next": true,
    "**/node_modules": false
  }
}
