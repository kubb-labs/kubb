{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "dependsOn": ["^clean"],
      "outputs": [],
      "cache": false,
      "outputLogs": "new-only"
    },
    "start": {
      "dependsOn": ["^start"],
      "outputs": ["dist/**"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint-fix": {
      "outputs": ["src/**"],
      "cache": false,
      "outputLogs": "new-only"
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "release": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "generate": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "release:canary": {
      "dependsOn": ["build"],
      "outputs": []
    }
  }
}
