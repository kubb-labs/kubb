{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "dependsOn": ["^clean"],
      "outputs": [],
      "cache": false,
      "outputMode": "new-only"
    },
    "start": {
      "dependsOn": ["^start"],
      "outputs": ["dist/**"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint-fix": {
      "outputs": ["src/**"],
      "cache": false,
      "outputMode": "new-only"
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "release": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "release:canary": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "generate": {
      "dependsOn": [],
      "outputs": [
        // "dist/**",
        // "gen/**",
        // "src/gen/**"
      ],
      "cache": false,
      "outputMode": "errors-only"
    }
  }
}
