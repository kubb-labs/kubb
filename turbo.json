{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "outputs": [],
      "cache": false,
      "outputMode": "new-only"
    },
    "start": {
      "dependsOn": [
        "^start"
      ],
      "outputs": [
        "dist/**"
      ],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [],
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint-fix": {
      "outputs": [
        "src/**"
      ],
      "cache": false,
      "outputMode": "new-only"
    },
    "typecheck": {
      "dependsOn": [],
      "outputs": [
        "dist/**"
      ],
      "cache": false
    },
    "release": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "release:canary": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "release:alpha": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "generate": {
      "dependsOn": [],
      "outputs": [
        // "dist/**",
        // "gen/**",
        // "src/gen/**"
      ],
      "cache": false,
      "outputMode": "errors-only"
    }
  }
}
