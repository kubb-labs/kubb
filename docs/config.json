{
  "$schema": "https://raw.githubusercontent.com/kubb-labs/kubb.dev/main/schemas/config/schema.json",
  "sidebars": [
    {
      "name": "default",
      "value": [
        {
          "text": "Menu",
          "items": [
            {
              "text": "Home",
              "link": "/",
              "activeMatch": "^/$"
            },
            {
              "text": "Github",
              "link": "https://github.com/kubb-labs/kubb",
              "target": "_blank"
            },
            {
              "text": "Releases",
              "link": "https://github.com/kubb-labs/kubb/releases",
              "target": "_blank"
            },
            {
              "text": "Changelog",
              "link": "/changelog",
              "activeMatch": "^/changelog/?$"
            },
            {
              "text": "Migration guide",
              "link": "/migration-guide",
              "activeMatch": "^/migration-guide/?$"
            }
          ]
        },
        {
          "text": "Getting started",
          "collapsed": false,
          "items": [
            {
              "text": "At Glance",
              "link": "/getting-started/at-glance",
              "activeMatch": "^/getting-started/at-glance/?$"
            },
            {
              "text": "Ecosystem",
              "link": "/getting-started/ecosystem",
              "activeMatch": "^/getting-started/ecosystem/?$"
            },
            {
              "text": "Quick Start",
              "link": "/getting-started/quick-start",
              "activeMatch": "^/getting-started/quick-start/?$"
            },
            {
              "text": "Configure",
              "link": "/getting-started/configure",
              "activeMatch": "^/getting-started/configure/?$"
            },
            {
              "text": "Troubleshooting",
              "link": "/getting-started/troubleshooting",
              "activeMatch": "^/getting-started/troubleshooting/?$"
            }
          ]
        },
        {
          "text": "Plugins",
          "collapsed": false,
          "items": [
            {
              "text": "@kubb/plugin-oas",
              "link": "/plugins/plugin-oas",
              "activeMatch": "^/plugins/plugin-oas/?$",
              "collapsed": true,
              "items": [
                {
                  "text": "Hooks",
                  "link": "/plugins/plugin-oas/hooks",
                  "activeMatch": "^/plugins/plugin-oas/hooks/?$"
                }
              ]
            },
            {
              "text": "@kubb/plugin-cypress <span class=\"new\">new in 3.7.0</span>",
              "link": "/plugins/plugin-cypress",
              "activeMatch": "^/plugins/plugin-cypress/?$"
            },
            {
              "text": "@kubb/plugin-mcp <span class=\"new\">new in 3.9.0</span>",
