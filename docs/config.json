            },
            {
              "text": "@kubb/cli",
              "link": "/helpers/cli",
              "activeMatch": "^/helpers/cli/?$"
            },
            {
              "text": "unplugin-kubb",
              "link": "/builders/unplugin",
              "activeMatch": "^/builders/unplugin/?$"
            }
          ]
        },
        {
          "text": "Playground",
          "link": "/playground",
          "activeMatch": "^/playground/?$"
        }
      ]
    }
  ],
  "sidebar": {
    "/getting-started": "default",
    "/plugins": "default",
    "/helpers": "default",
    "/build": "default",
    "/changelog": "default",
    "/examples": "default",
    "/migration-guide": "default",
    "/tutorials": "default",
    "/playground": "default",
    "/knowledge-base": "knowledgeBase"
