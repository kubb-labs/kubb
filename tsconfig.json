{
  "extends": "@kubb/config-ts/bundler.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["vitest/globals", "bun-types", "@kubb/config-ts"],
    "paths": {
      "@kubb/core/utils": ["packages/core/src/utils/index.ts"],
      "@kubb/core/transformers": ["packages/core/src/transformers/index.ts"],
      "@kubb/core/fs": ["packages/core/src/fs/index.ts"],
      "@kubb/core/mocks": ["packages/core/mocks/index.ts"],
      "@kubb/core/transformer": ["packages/core/transformer/index.ts"],
      "@kubb/core": ["packages/core/src/index.ts"],
      "@kubb/types": ["packages/types/src/index.ts"],
      "@kubb/plugin-oas/hooks": ["packages/plugin-oas/src/hooks/index.ts"],
      "@kubb/plugin-oas/utils": ["packages/plugin-oas/src/utils/index.ts"],
      "@kubb/plugin-oas/components": ["packages/plugin-oas/src/components/index.ts"],
      "@kubb/plugin-oas": ["packages/plugin-oas/src/index.ts"],
      "@kubb/swagger": ["packages/swagger/src/index.ts"],
      "@kubb/swagger-ts/hooks": ["packages/swagger-ts/src/hooks/index.ts"],
      "@kubb/swagger-ts": ["packages/swagger-ts/src/index.ts"],
      "@kubb/react/jsx-runtime": ["packages/react/src/jsx-runtime.ts"],
      "@kubb/swagger-client": ["packages/swagger-client/src/index.ts"],
      "@kubb/swagger-client/client": ["packages/swagger-client/src/client.ts"],
      "@kubb/swagger-client/components": ["packages/swagger-client/src/components/index.ts"],
      "@kubb/plugin-redoc": ["packages/plugin-redoc/src/index.ts"],
      "@kubb/react/server": ["packages/react/src/server/index.ts"],
      "@kubb/react": ["packages/react/src/index.ts"],
      "@kubb/parser-ts/factory": ["packages/parser-ts/src/factory.ts"],
      "@kubb/parser-ts": ["packages/parser-ts/src/index.ts"],
      "@kubb/oas": ["packages/oas/src/index.ts"],
      "@kubb/oas/parser": ["packages/oas/src/parser/index.ts"],
      "unplugin-kubb": ["packages/unplugin/src/index.ts"]
    }
  },
  "include": ["./package.json", "./tsup.config.ts", "./vitest.config.ts"],
  "exclude": ["**/node_modules/**", "**/__snapshots__/**"]
}
