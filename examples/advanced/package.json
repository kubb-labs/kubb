{
  "name": "advanced-pet-store",
  "version": "0.0.1",
  "description": "Advanced PetStore example",
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/examples/advanced"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "generate": "kubb --config kubb.config.ts",
    "generate:js": "kubb --config .kubbrc",
    "generate:json": "kubb --config kubb.json",
    "generate:ts": "kubb --config kubb.config.ts",
    "start": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@kubb/cli": "workspace:*",
    "@kubb/core": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-client": "workspace:*",
    "@kubb/swagger-faker": "workspace:*",
    "@kubb/swagger-swr": "workspace:*",
    "@kubb/swagger-tanstack-query": "workspace:*",
    "@kubb/swagger-ts": "workspace:*",
    "@kubb/swagger-zod": "workspace:*",
    "@kubb/swagger-zodios": "workspace:*",
    "@tanstack/react-query": "^4.29.14",
    "@tanstack/solid-query": "^4.29.14",
    "@tanstack/svelte-query": "^4.29.14",
    "@tanstack/vue-query": "^4.29.14",
    "@zodios/core": "^10.9.2",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "solid-js": "^1.7.6",
    "svelte": "^3.59.1",
    "swr": "^2.1.5",
    "tsup": "^7.0.0",
    "vue": "^3.3.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@kubb/ts-config": "workspace:*",
    "typescript": "^5.1.3"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  }
}