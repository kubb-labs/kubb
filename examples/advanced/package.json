    "@tanstack/vue-query": "^5.89.0",
    "@types/react": "^19.1.13",
    "axios": "^1.12.2",
    "cypress": "~14.3.3",
    "msw": "^2.11.2",
    "react": "^19.1.1",
    "solid-js": "^1.9.9",
    "svelte": "^4.2.20",
    "swr": "^2.3.6",
    "vue": "^3.5.21",
