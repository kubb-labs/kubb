    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-ts": "workspace:*",
    "axios": "^1.4.0",
    "tsup": "^7.1.0"
  },
  "devDependencies": {
    "typescript": "^5.1.3"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}