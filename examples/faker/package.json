{
  "name": "faker-pet-store",
  "version": "0.0.1",
  "description": "Faker PetStore example",
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/examples/faker"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "generate": "kubb",
    "generate:js": "kubb --config .kubbrc",
    "generate:json": "kubb --config kubb.json",
    "generate:ts": "kubb --config kubb.config.js",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@kubb/cli": "workspace:*",
    "@kubb/core": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-client": "workspace:*",
    "@kubb/swagger-faker": "workspace:*",
    "@kubb/swagger-ts": "workspace:*",
    "react": "^18.2.0",
    "tsup": "^6.7.0"
  },
  "devDependencies": {
    "@kubb/typescript-config": "workspace:*",
    "typescript": "^5.1.3"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  }
}