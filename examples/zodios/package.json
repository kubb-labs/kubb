{
  "name": "zodios-pet-store",
  "version": "0.0.1",
  "description": "zodios PetStore example",
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/examples/zodios"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "generate": "pnpm generate:ts",
    "generate:js": "kubb --config .kubbrc",
    "generate:json": "kubb --config kubb.json",
    "generate:ts": "kubb --config kubb.config.js",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/cli": "workspace:*",
    "@kubb/core": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-client": "workspace:*",
    "@kubb/swagger-zod": "workspace:*",
    "@kubb/swagger-zodios": "workspace:*",
    "@zodios/core": "^10.9.0",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "tsup": "^6.7.0",
    "zod": "^3.21.4"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  }
}