{
  "name": "@kubb/swagger-zod",
  "version": "2.18.4",
  "description": "Generator swagger-zod",
  "keywords": [
    "zod",
    "plugins",
    "kubb",
    "codegen",
    "swagger",
    "openapi"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-labs/kubb.git",
    "directory": "packages/swagger-zod"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs",
      "default": "./dist/components.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "npx rimraf ./dist",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --apply-unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "@kubb/oas": "workspace:*",
    "@kubb/fs": "workspace:*",
    "@kubb/parser-ts": "workspace:*",
    "@kubb/plugin-oas": "workspace:*",
    "@kubb/react": "workspace:*",
    "@kubb/swagger-ts": "workspace:*"
  },
  "devDependencies": {
    "@kubb/config-biome": "workspace:*",
    "@kubb/config-ts": "workspace:*",
    "@kubb/config-tsup": "workspace:*",
    "@types/react": "^18.3.1",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@kubb/react": "workspace:*"
  },
  "packageManager": "pnpm@9.0.5",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.15.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
