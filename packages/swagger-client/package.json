{
  "name": "@kubb/swagger-client",
  "version": "1.2.1",
  "description": "Generator swagger-client",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "codegen",
    "swagger",
    "openapi",
    "axios"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/swagger-client"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "module": "./dist/client.js",
      "default": "./dist/client.cjs"
    },
    "./ts-client": {
      "types": "./dist/client.d.ts",
      "import": "./client.ts",
      "module": "./client.ts",
      "default": "./client.ts"
    },
    "./package.json": "./package.json",
    "./*": "./*",
    "./globals": {
      "types": "./globals.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client.d.ts"
      ],
      "ts-client": [
        "./dist/client.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "client.ts",
    "*.d.ts",
    "*.d.cts",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint --format pretty ./src",
    "lint-fix": "bun run lint --quiet --fix",
    "release": "pnpm publish --no-git-check",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-ts": "workspace:*",
    "@kubb/ts-codegen": "workspace:*",
    "change-case": "^4.1.2"
  },
  "devDependencies": {
    "@kubb/eslint-config": "workspace:*",
    "@kubb/ts-config": "workspace:*",
    "@kubb/tsup-config": "workspace:*",
    "axios": "^1.4.0",
    "eslint": "^8.42.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "axios": "^1.4.0"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
