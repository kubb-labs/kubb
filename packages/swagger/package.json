{
  "name": "@kubb/swagger",
  "version": "1.0.1-alpha.7",
  "description": "Generator swagger",
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/swagger"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "codegen",
    "swagger",
    "openapi"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "release": "pnpm publish --no-git-check",
    "prepack": "pnpm build",
    "pre-commit": "echo 'pre-commit not configured'",
    "pre-push": "pnpm typecheck",
    "test": "jest --config ../../jest.config.ts --passWithNoTests --runInBand --testPathPattern=packages/swagger/src",
    "test:local": "jest --config ../../jest.config.ts --testPathPattern=packages/swagger/src --maxWorkers=50%",
    "test:watch": "jest --config ../../jest.config.ts --testPathPattern=packages/swagger/src --maxWorkers=25% --watch",
    "upgrade": "ncu -u",
    "upgrade:local": "ncu --interactive --doctor",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@kubb/core": "workspace:*",
    "change-case": "^4.1.2",
    "lodash.get": "^4.4.2",
    "oas": "^20.5.3",
    "oas-normalize": "^8.3.3",
    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/swagger2openapi": "^7.0.0",
    "openapi-types": "^12.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": "^12.17.0 || ^14.13 || >=16.0.0"
  }
}
