{
  "name": "@kubb/core",
  "version": "0.37.10",
  "description": "Generator core",
  "repository": {
    "type": "git",
    "url": "git://github.com/stijnvanhulle/kubb.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "codegen"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist",
    "schemas",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "release": "pnpm publish --no-git-check",
    "prepack": "pnpm build",
    "pre-commit": "echo 'pre-commit not configured'",
    "pre-push": "pnpm typecheck",
    "test": "jest --config ../../jest.config.js --passWithNoTests --runInBand --testPathPattern=packages/core/src",
    "test:local": "jest --config ../../jest.config.js --testPathPattern=packages/core/src --maxWorkers=50%",
    "test:watch": "jest --config ../../jest.config.js --testPathPattern=packages/core/src --maxWorkers=25% --watch",
    "upgrade": "ncu -u",
    "upgrade:local": "ncu --interactive --doctor",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "directory-tree": "^3.5.1",
    "eventemitter3": "^5.0.0",
    "fs-extra": "^11.1.0",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "crypto-browserify": "^3.12.0",
    "memfs": "^3.4.13",
    "openapi-types": "^12.1.0",
    "path-browserify": "^1.0.1",
    "tsup": "^6.5.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
