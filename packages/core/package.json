{
  "name": "@kubb/core",
  "version": "2.12.6",
  "description": "Generator core",
  "keywords": ["typescript", "plugins", "kubb", "codegen"],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "default": "./dist/utils.cjs"
    },
    "./transformers": {
      "import": "./dist/transformers.js",
      "require": "./dist/transformers.cjs",
      "default": "./dist/transformers.cjs"
    },
    "./logger": {
      "import": "./dist/logger.js",
      "require": "./dist/logger.cjs",
      "default": "./dist/logger.cjs"
    },
    "./fs": {
      "import": "./dist/fs.js",
      "require": "./dist/fs.cjs",
      "default": "./dist/fs.cjs"
    },
    "./mocks": {
      "import": "./dist/mocks.js",
      "require": "./dist/mocks.cjs",
      "default": "./dist/mocks.cjs"
    },
    "./package.json": "./package.json",
    "./globals": {
      "types": "./globals.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "utils": ["./dist/utils.d.ts"],
      "transformers": ["./dist/transformers.d.ts"],
      "logger": ["./dist/logger.d.ts"],
      "fs": ["./dist/fs.d.ts"],
      "mocks": ["./dist/mocks.d.ts"]
    }
  },
  "files": ["src", "dist", "*.d.ts", "*.d.cts", "schema.json", "!/**/**.test.**", "!/**/__tests__/**"],
  "scripts": {
    "build": "tsup",
    "clean": "npx rimraf ./dist",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --apply-unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/parser": "workspace:*",
    "@kubb/types": "workspace:*",
    "change-case": "^5.4.4",
    "directory-tree": "^3.5.1",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "js-runtime": "^0.0.7",
    "natural-orderby": "^3.0.2",
    "p-queue": "^8.0.1",
    "remeda": "^1.58.2",
    "seedrandom": "^3.0.5",
    "semver": "^7.6.0",
    "unraw": "^3.0.0"
  },
  "devDependencies": {
    "@kubb/biome-config": "workspace:*",
    "@kubb/ts-config": "workspace:*",
    "@kubb/tsup-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.2.78",
    "@types/seedrandom": "^3.0.8",
    "@types/semver": "^7.5.8",
    "ora": "^8.0.1",
    "prettier": "^3.2.5",
    "tinyrainbow": "^1.1.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.15.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
