{
  "name": "@kubb/swagger-ts",
  "version": "2.12.5",
  "description": "Generator swagger-ts",
  "keywords": ["typescript", "plugins", "kubb", "codegen", "swagger", "openapi"],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/swagger-ts"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs",
      "default": "./dist/components.cjs"
    },
    "./oas": {
      "types": "./dist/oas.d.ts"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "components": ["./dist/components.d.ts"],
      "oas": ["./dist/oas.d.ts"]
    }
  },
  "files": ["src", "dist", "!/**/**.test.**", "!/**/__tests__/**"],
  "scripts": {
    "build": "tsup",
    "clean": "npx rimraf ./dist",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --apply-unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "@kubb/parser": "workspace:*",
    "@kubb/react": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/types": "workspace:*",
    "hotscript": "^1.0.13",
    "json-schema-to-ts": "^3.0.1",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@kubb/biome-config": "workspace:*",
    "@kubb/ts-config": "workspace:*",
    "@kubb/tsup-config": "workspace:*",
    "@types/react": "^18.2.74",
    "expect-type": "^0.19.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "@kubb/react": "workspace:*"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.15.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
