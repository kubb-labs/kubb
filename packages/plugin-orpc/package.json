{
  "name": "@kubb/plugin-orpc",
  "version": "4.18.4",
  "description": "oRPC contract generator plugin for Kubb, creating type-safe API contracts from OpenAPI specifications.",
  "keywords": [
    "orpc",
    "contract",
    "api",
    "type-safety",
    "typescript",
    "openapi",
    "swagger",
    "code-generator",
    "codegen",
    "plugins",
    "kubb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kubb-labs/kubb.git",
    "directory": "packages/plugin-orpc"
  },
  "license": "MIT",
  "author": "stijnvanhulle",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./generators": {
      "import": "./dist/generators.js",
      "require": "./dist/generators.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components.d.ts"
      ],
      "generators": [
        "./dist/generators.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "size-limit": [
    {
      "path": "./dist/*.js",
      "limit": "510 KiB",
      "gzip": true
    }
  ],
  "scripts": {
    "build": "tsdown && size-limit",
    "clean": "npx rimraf ./dist",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --fix --unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "start": "tsdown --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "@kubb/oas": "workspace:*",
    "@kubb/plugin-oas": "workspace:*",
    "@kubb/plugin-zod": "workspace:*",
    "@kubb/react-fabric": "catalog:"
  },
  "devDependencies": {
    "@orpc/contract": "^1.4.3",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@orpc/contract": "^1.0.0",
    "zod": "^3.23.0"
  },
  "peerDependenciesMeta": {
    "@orpc/contract": {
      "optional": false
    },
    "zod": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
