{
  "name": "@kubb/agent",
  "version": "4.29.1",
  "description": "Agent server for Kubb, enabling HTTP-based access to code generation capabilities.",
  "keywords": [
    "agent",
    "server",
    "http",
    "kubb",
    "code-generation",
    "openapi",
    "swagger",
    "typescript",
    "codegen"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubb-labs/kubb.git",
    "directory": "packages/agent"
  },
  "license": "MIT",
  "author": "stijnvanhulle",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "files": [
    ".output",
    "!/**/**.test.**",
    "!/**/__tests__/**",
    "!/**/__snapshots__/**"
  ],
  "scripts": {
    "build": "nitro build",
    "clean": "npx rimraf ./dist .output",
    "prepare": "nitro prepare",
    "lint": "bun biome lint .",
    "start": "node .output/server/index.mjs",
    "lint:fix": "bun biome lint --fix --unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "dev": "nitro dev",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "@kubb/fabric-core": "catalog:",
    "@kubb/plugin-client": "workspace:*",
    "@kubb/plugin-cypress": "workspace:*",
    "@kubb/plugin-faker": "workspace:*",
    "@kubb/plugin-mcp": "workspace:*",
    "@kubb/plugin-msw": "workspace:*",
    "@kubb/plugin-oas": "workspace:*",
    "@kubb/plugin-react-query": "workspace:*",
    "@kubb/plugin-redoc": "workspace:*",
    "@kubb/plugin-solid-query": "workspace:*",
    "@kubb/plugin-svelte-query": "workspace:*",
    "@kubb/plugin-swr": "workspace:*",
    "@kubb/plugin-ts": "workspace:*",
    "@kubb/plugin-vue-query": "workspace:*",
    "@kubb/plugin-zod": "workspace:*",
    "jiti": "2.5.1",
    "tinyexec": "catalog:",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "msw": "^2.12.10",
    "nitropack": "^2.13.1",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
