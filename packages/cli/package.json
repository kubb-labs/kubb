{
  "name": "@kubb/cli",
  "version": "1.0.0-alpha.1",
  "description": "Generator cli",
  "repository": {
    "type": "git",
    "url": "git://github.com/stijnvanhulle/kubb.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "codegen",
    "cli"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kubb": "dist/index.js"
  },
  "preferGlobal": true,
  "files": [
    "dist",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "release": "pnpm publish --no-git-check",
    "prepack": "pnpm build",
    "pre-commit": "echo 'pre-commit not configured'",
    "pre-push": "pnpm typecheck",
    "test": "jest --config ../../jest.config.ts --passWithNoTests --runInBand --testPathPattern=packages/cli/src",
    "test:local": "jest --config ../../jest.config.ts --testPathPattern=packages/cli/src --maxWorkers=50%",
    "test:watch": "jest --config ../../jest.config.ts --testPathPattern=packages/cli/src --maxWorkers=25% --watch",
    "upgrade": "ncu -u",
    "upgrade:local": "ncu --interactive --doctor",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@humanwhocodes/module-importer": "^1.0.1",
    "@kubb/core": "workspace:*",
    "chokidar": "^3.5.3",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "execa": "^7.0.0",
    "ora": "^6.1.2",
    "picocolors": "^1.0.0",
    "string-argv": "^0.3.1",
    "ts-node": "^10.9.1",
    "@swc/core": "^1.3.38"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": "^12.17.0 || ^14.13 || >=16.0.0"
  }
}
