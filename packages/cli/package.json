{
  "name": "@kubb/cli",
  "version": "0.37.9",
  "description": "Generator cli",
  "repository": {
    "type": "git",
    "url": "git://github.com/stijnvanhulle/kubb.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "codegen",
    "cli"
  ],
  "sideEffects": false,
  "bin": {
    "kubb": "dist/index.mjs"
  },
  "preferGlobal": true,
  "files": [
    "dist",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "release": "pnpm publish --no-git-check",
    "prepack": "pnpm build",
    "pre-commit": "echo 'pre-commit not configured'",
    "pre-push": "pnpm typecheck",
    "test": "jest --config ../../jest.config.js --passWithNoTests --runInBand --testPathPattern=packages/core/src",
    "test:local": "jest --config ../../jest.config.js --testPathPattern=packages/core/src --maxWorkers=50%",
    "test:watch": "jest --config ../../jest.config.js --testPathPattern=packages/core/src --maxWorkers=25% --watch",
    "upgrade": "ncu -u",
    "upgrade:local": "ncu --interactive --doctor",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "chokidar": "^3.5.3",
    "commander": "^9.4.1",
    "cosmiconfig": "^8.0.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "execa": "^6.1.0",
    "ora": "^6.1.2",
    "picocolors": "^1.0.0",
    "string-argv": "^0.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
