{
  "name": "@kubb/cli",
  "version": "1.1.0",
  "description": "Generator cli",
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "keywords": [
    "typescript",
    "plugins",
    "kubb",
    "codegen",
    "cli"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kubb": "dist/index.js"
  },
  "preferGlobal": true,
  "files": [
    "dist",
    "src",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "release": "pnpm publish --no-git-check",
    "prepack": "pnpm build",
    "pre-commit": "echo 'pre-commit not configured'",
    "pre-push": "pnpm typecheck",
    "test": "vitest --passWithNoTests",
    "upgrade": "ncu -u",
    "upgrade:local": "ncu --interactive --doctor",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "chokidar": "^3.5.3",
    "commander": "^10.0.1",
    "cosmiconfig": "^8.2.0",
    "execa": "^7.1.1",
    "ora": "^6.3.1",
    "picocolors": "^1.0.0",
    "string-argv": "^0.3.2",
    "ts-node": "^10.9.1",
    "yaml": "^2.3.1",
    "@swc/core": "^1.3.62",
    "pretty-error": "^4.0.0"
  },
  "devDependencies": {
    "@kubb/swagger": "workspace:*",
    "@types/node": "^20.2.5",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  }
}
