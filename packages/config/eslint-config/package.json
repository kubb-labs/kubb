{
  "name": "@kubb/eslint-config",
  "version": "1.1.8",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "module": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./flat": {
      "import": "./dist/flat.js",
      "module": "./dist/flat.js",
      "default": "./dist/flat.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "flat": [
        "./dist/flat.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "schemas",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf ./dist",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint --format pretty .",
    "lint-fix": "bun run lint --quiet --fix",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../../.github/canary.sh && pnpm publish --no-git-check -tag canary",
    "start": "tsup --watch",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@eslint/js": "^8.49.0",
    "@types/eslint": "~8.44.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "~6.6.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-config-turbo": "~1.10.13",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-unused-imports": "~3.0.0",
    "eslint-plugin-vitest": "~0.3.1",
    "eslint-plugin-vitest-globals": "~1.4.0",
    "globals": "^13.21.0"
  },
  "devDependencies": {
    "@kubb/ts-config": "workspace:*",
    "@kubb/tsup-config": "workspace:*",
    "eslint": "~8.48.0",
    "tsup": "^7.2.0",
    "typescript": "~5.2.2"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
