{
  "name": "@kubb/swagger-zodios",
  "version": "2.6.1",
  "description": "Generator swagger-zodios",
  "keywords": [
    "zod",
    "zodios",
    "plugins",
    "kubb",
    "codegen",
    "swagger",
    "openapi"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kubb-project/kubb.git",
    "directory": "packages/swagger-zodios"
  },
  "license": "MIT",
  "author": "Stijn Van Hulle <stijn@stijnvanhulle.be",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs",
      "default": "./dist/components.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "npx rimraf ./dist",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint --format pretty .",
    "lint-fix": "bun run lint --quiet --fix",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@kubb/core": "workspace:*",
    "@kubb/parser": "workspace:*",
    "@kubb/react": "workspace:*",
    "@kubb/swagger": "workspace:*",
    "@kubb/swagger-zod": "workspace:*"
  },
  "devDependencies": {
    "@kubb/eslint-config": "workspace:*",
    "@kubb/ts-config": "workspace:*",
    "@kubb/tsup-config": "workspace:*",
    "@types/react": "^18.2.56",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "peerDependencies": {
    "@kubb/react": "workspace:*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
