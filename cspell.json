{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-US",
  "useGitignore": true,
  "ignorePaths": [
    "**/*.snap",
    "__snapshots__",
    "mocks",
    "**/*.d.ts",
    "*.lock",
    "pnpm-lock.yaml",
    "**/.changeset/**",
    "**/CHANGELOG.md",
    "**/changelog.md",
    "README.md",
    "examples/**/src/gen/**",
    "examples/**/src/gen2/**",
    "examples/**/src/gen3/**",
    "examples/**/kubb-gen/**",
    "tests/**/gen/**",
    "tests/e2e/schemas/**",
    "schemas/**",
    "docs/examples/**",
    "docs/playground.md",
    "docs/guide/fetch.md",
    "docs/guide/base-url.md",
    "docs/ecosystem.md",
    "docs/about.md"
  ],
  "ignoreRegExpList": ["https?://github\\.com/[a-zA-Z0-9-_]+(/[a-zA-Z0-9-_]+)?", "\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Z|a-z]{2,}\\b", "\\[kub[ːˑ]+\\]"],
  "words": [
    "stijnvanhulle",
    "Kubb",
    "kubbar",
    "kubbrc",
    "unplugin",
    "Unplugin",
    "Turborepo",
    "Rspack",
    "Extname",
    "citty",
    "OpenAPI",
    "tsoa",
    "Hoppscotch",
    "zodios",
    "Writables",
    "twoslash",
    "jsonpointer",
    "petstore.yaml",
    "modelcontextprotocol",
    "x-enumNames",
    "x-enum-varnames",
    "Vitest",
    "igmsuy",
    "randexp",
    "redocly",
    "Vitepress",
    "Biome",
    "Tanstack",
    "mutationkey",
    "MSW",
    "Faker",
    "Zod",
    "Cypress",
    "Axios",
    "datetime",
    "IBAN",
    "oas",
    "monorepo",
    "APIV",
    "dedupe",
    "Oxlint",
    "Oxfmt",
    "whatwg",
    "Github",
    "endgroup"
  ]
}
